import java.util.Scanner;

public class PalavraArara {
    public static void main(String[] args) {
        // Criando o scanner para ler a entrada do usuário
        Scanner scanner = new Scanner(System.in);
        
        // Lê as letras 'a' e 'r'
        char letra1 = scanner.next().charAt(0);
        char letra2 = scanner.next().charAt(0);

        // Imprime a palavra "arara"
        System.out.printf("%c%c%c%c%c\n", letra1, letra2, letra1, letra2, letra1);
        
        // Fecha o scanner
        scanner.close();
    }
}
